ode.dt  = 1.e-05
ode.ndt = 1
# Reactor formalism: 1=full e, 2=full h
ode.reactor_type  = 1
#verbose
ode.verbose = 5
# abs and rel tolerances for ODE solve
#ode.rtol = 1e-10
#ode.atol = 1e-10
